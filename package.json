{
  "name": "translator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "jest --coverage --env=node",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "datocms-client": "^3.5.14",
    "datocms-plugin-sdk": "^0.5.2",
    "datocms-react-ui": "^0.5.2",
    "humps": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "datoCmsPlugin": {
    "title": "DatoCMS translator",
    "previewImage": "",
    "coverImage": "",
    "entryPoint": "build/index.html"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]}
}
